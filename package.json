        "auth",
        "ui",
        "vue",
        "auth-ui",
        "authenticate",
        "auth-helper"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Boluwatife2904/auth-ui-vue"
    },
    "license": "MIT",
    "publishConfig": {
