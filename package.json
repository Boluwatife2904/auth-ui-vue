        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "sass": "^1.56.2",
        "typescript": "~4.7.4",
        "vite": "^3.2.4",
        "vite-plugin-vue-type-imports": "^0.2.4"
    }
