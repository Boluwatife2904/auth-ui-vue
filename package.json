    },
    "files": [
        "dist"
    ],
    "main": "./dist/@supabase/auth-ui-vue.umd.js",
    "module": "./dist/@supabase/auth-ui-vue.es.js",
    "exports": {
        ".": {
            "import": "./dist/@supabase/auth-ui-vue.es.js",
