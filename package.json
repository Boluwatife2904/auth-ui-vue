{
    "name": "auth-ui-vue",
    "version": "0.1.0",
    "author": {
        "email": "sanvicola2000@gmail.com",
        "name": "Sanusi Victor Olajuwon",
        "url": "https://sanusi-victor.netlify.app"
    },
    "description": "Supabase Auth UI is a pre-built Vue component for authenticating users. It supports custom themes and extensible styles to match your brand and aesthetic.",
    "keywords": [
        "supabase",
        "auth",
        "ui",
        "vue",
        "auth-ui",
        "authenticate",
        "auth-helper"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Boluwatife2904/auth-ui-vue"
    },
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/auth-ui-vue.umd.js",
    "module": "./dist/auth-ui-vue.es.js",
    "exports": {
        ".": {
            "import": "./dist/auth-ui-vue.es.js",
            "require": "./dist/auth-ui-vue.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.2.0",
        "@supabase/supabase-js": "^2.21.0",
        "@types/node": "^18.16.0",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.3.2",
        "eslint": "^8.39.0",
        "eslint-plugin-vue": "^9.11.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "rollup-plugin-typescript2": "^0.34.1",
        "sass": "^1.62.0",
        "typescript": "~5.0.4",
        "vite": "^4.3.1",
        "vite-plugin-vue-type-imports": "^0.2.4",
        "vue-tsc": "^1.4.4"
    }
}
